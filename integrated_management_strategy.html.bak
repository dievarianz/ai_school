  /* Spotlight Teaser */
  .teaser-spotlight {
    position: relative;
    background: linear-gradient(120deg, rgba(110, 69, 226, 0.1), rgba(156, 39, 176, 0.1));
    border-radius: 6px;
    padding: 8px 15px;
    margin-bottom: 15px;
    font-size: 0.85rem;
    font-weight: 500;
    color: var(--imt-purple);
    text-align: center;
    overflow: hidden;
  }
  
  .teaser-spotlight::before {
    content: "";
    position: absolute;
    top: -50%;
    left: -50%;
    width: 200%;
    height: 200%;
    background: radial-gradient(circle, rgba(255,255,255,0.8) 0%, rgba(255,255,255,0) 60%);
    opacity: 0.6;
    animation: spotlight 3s infinite linear;
    pointer-events: none;
  }
  
  @keyframes spotlight {
    0% { transform: translate(-30%, -30%) rotate(0deg); }
    100% { transform: translate(-30%, -30%) rotate(360deg); }
  }

  /* Bubble Teaser Style */
  .teaser-bubble {
    display: inline-block;
    position: relative;
    background-color: var(--imt-purple);
    color: white;
    padding: 8px 15px;
    border-radius: 18px;
    font-size: 0.85rem;
    font-weight: 500;
    margin-bottom: 25px;
    text-align: center;
  }
  
  .teaser-bubble::after {
    content: "";
    position: absolute;
    bottom: -8px;
    left: 20px;
    width: 15px;
    height: 15px;
    background-color: var(--imt-purple);
    transform: rotate(45deg);
    clip-path: polygon(0% 0%, 100% 100%, 0% 100%);
  }

  /* Floating Teaser Style */
  .teaser-floating {
    position: absolute;
    top: -15px;
    left: 0;
    right: 0;
    text-align: center;
  }
  
  .teaser-floating span {
    display: inline-block;
    background: linear-gradient(135deg, var(--imt-purple), #9c27b0);
    color: white;
    padding: 5px 15px;
    border-radius: 20px;
    font-size: 0.75rem;
    font-weight: 500;
    box-shadow: 0 2px 8px rgba(0,0,0,0.15);
  }
  
  /* Tag Teaser Style */
  .teaser-tag {
    display: inline-flex;
    align-items: center;
    border: 1px solid var(--imt-purple);
    color: var(--imt-purple);
    padding: 4px 10px;
    border-radius: 4px;
    font-size: 0.75rem;
    font-weight: 500;
    margin-bottom: 15px;
  }
  
  /* Punkt für "Nur noch wenige Plätze verfügbar" - für "Managementseminar" deaktiviert
  .teaser-tag::before {
    content: "";
    display: inline-block;
    width: 6px;
    height: 6px;
    border-radius: 50%;
